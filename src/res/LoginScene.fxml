<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.PasswordField?>

<?import javafx.scene.control.Label?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LoginController">
    <AnchorPane VBox.vgrow="ALWAYS">
        <Label fx:id="incorrectPassowordLabel" layoutX="181.0" layoutY="200"/>
        <TextField fx:id="username" layoutX="181.0" layoutY="114.0" prefHeight="27.0" prefWidth="279.0" promptText="Username" />
        <PasswordField fx:id="password" layoutX="181.0" layoutY="173.0" prefHeight="27.0" prefWidth="279.0" promptText="Password" onAction="#connectToDatabase"/>
    </AnchorPane>
</VBox>
